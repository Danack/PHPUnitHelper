{
    "name": "danack/php-unit-helper",
    "description": "Small set of functions to use within PHPUnit.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Danack",
            "email": "Danack@basereality.com"
        }
    ],
    "require": {
        "php": ">=7.4",
        "danack/coding-standard": "0.2.0",
        "squizlabs/php_codesniffer": "^3.3.1",
        "yoast/yoastcs": "1.0",
        "phpunit/phpunit": "9.0.1"
    },
    "autoload": {
        "files": [
            "src/Danack/PHPUnitHelper/functions.php"
        ],
        "psr-0": {
            "Danack\\PHPUnitHelper\\": "src/"
